               .modal-all-page.modal {
                   position: absolute;
                   top: 0;
                   left: 0;
                   visibility: hidden;
                   width: 100%;
                   height: 100%
               }

               .modal-all-page.modal .modal-overlay {
                   position: fixed;
                   z-index: 8;
                   top: 0;
                   left: 0;
                   width: 100vw;
                   height: 100vh;
                   visibility: hidden;
                   opacity: 0;
                   --bs-backdrop-bg: #000;
                   background-color: var(--bs-backdrop-bg);
                   transition: visibility 0s linear .3s,opacity .3s
               }

               .modal-all-page.modal .modal-transition {
                   transition: all .3s .12s;
                   transform: translateX(10%);
                   opacity: 0
               }

               .modal-all-page.modal.is-visible {
                   visibility: visible;
                   display: block
               }

               .modal-all-page.modal.is-visible .modal-overlay {
                   --bs-backdrop-opacity: 0.5;
                   opacity: var(--bs-backdrop-opacity);
                   visibility: visible;
                   transition-delay: 0s
               }

               .modal-all-page.modal.is-visible .modal-transition {
                   transform: translateX(0);
                   opacity: 1
               }

               .modal-all-page.modal .modal-wrapper {
                   position: fixed;
                   left: 0;
                   right: 0;
                   margin: 0 auto;
                   z-index: 9;
                   display: flex;
                   justify-content: center;
                   align-items: center;
                   flex-direction: column;
                   justify-self: center;
                   background-color: transparent;
                   border-radius: 0;
                   box-shadow: none;
                   top: calc(50% - 212px);
                   max-width: 480px;
                   padding: 18px;

               }

               .modal-all-page.modal .modal-wrapper .modal-header,.modal-all-page.modal .modal-wrapper .modal-content {
                   padding: 0px;
                   min-width: 100%
               }

               .modal-all-page.modal .modal-wrapper .modal-header {
                 border: 0px;
                   display: flex;
                   align-items: center;
                   justify-content: center;
                   position: relative
               }

               .modal-all-page.modal .modal-wrapper .modal-close {
                   position: absolute;
            top: -8px;
                   right: -8px;
                   width: 18px;
                   height: 18px;
                   cursor: pointer;
                   border-radius: 50%;
                   background-color: var(--color-white);
                   z-index: 1;
                   padding: 0
               }
            .modal-all-page.modal .modal-wrapper .modal-close svg{
              width: 100%;
              height: 100%;
            }

                           @media (min-width: 768px) {
                               .modal-all-page.modal .modal-wrapper .modal-close { top: -10px;
                   right: -10px;
                   width: 24px;
                   height: 24px;
                               }
                           }

               .modal-all-page.modal .modal-wrapper .modal-body {
                   width: 100%
               }

               .modal-all-page.modal .modal-wrapper .modal-body .modal-content img {
                   border-radius: 0px
               }
